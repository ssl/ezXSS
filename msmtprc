## This file is used by docker for easy external SMTP email configuration
## We suggest a free email service like Mailjet if you decide to use this
account default
host smtp.example.com
port 587
tls on
tls_starttls on
auth on
user usernamegoeshere
password passwordgoeshere
from ezXSS@example.com
logfile /var/log/msmtp.log
